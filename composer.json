{
    "name": "industrious/mailchimp",
    "description": "A MailChimp API consumer package.",
    "type": "library",
    "keywords": ["mailchimp", "newsletter"],
    "homepage": "https://github.com/industrious-mouse/mailchimp",
    "license": "MIT",
    "authors": [
        {
            "name": "Connor S. Parks",
            "email": "connor@industrious.agency",
            "homepage": "http://github.com/ConnorVG"
        }
    ],
    "support": {
        "source": "https://github.com/industrious-mouse/mailchimp",
        "issues": "https://github.com/industrious-mouse/mailchimp/issues"
    },
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Industrious\\MailChimp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Industrious\\Tests\\MailChimp\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
